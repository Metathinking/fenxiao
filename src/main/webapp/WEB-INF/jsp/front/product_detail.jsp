    // 获取url参数
    function getUrlQueryString(name){
        var reg = new RegExp("(^|&)"+ name +"=([^&]*)(&|$)");
        var r = window.location.search.substr(1).match(reg);
        if(r!= null){
            return  unescape(r[2]);
        }
        return null;
    }
    // 页面错误提示
    function errnoIng(){
        // 获取参数errno
